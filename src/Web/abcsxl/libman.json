{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "jsdelivr",
      "library": "startbootstrap-clean-blog@6.0.8",
      "destination": "wwwroot/lib/startbootstrap-clean-blog/",
      "files": [
        "dist/css/styles.css",
        "dist/css/styles.min.css",
        "dist/js/scripts.js",
        "dist/js/scripts.min.js"
      ]
    }
]
}